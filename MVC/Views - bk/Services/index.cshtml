}

<!--=== Content Part ===-->
<div class="container content">
    <div class="row margin-bottom-20">
        <div class="col-md-12">
            <p>
                @Html.Partial("~/Views/Services/Intro.cshtml")
            </p>
        </div>
    </div>
    @Html.Partial("~/Views/Services/Services.cshtml")


    <!-- Our Clients -->
    @Html.Partial("~/Views/Services/Clients.cshtml")
    <!--/flexslider-->
    <!-- //End Our Clients -->
</div>
<!--/container-->
