@section styles{
    <link rel="stylesheet" href="~/_Assets/plugins/flexslider/flexslider.css" type="text/css" media="screen" />
}
@section Scripts{
 <!-- JS Implementing Plugins -->
<script type="text/javascript" src="~/_Assets/plugins/flexslider/jquery.flexslider-min.js"></script>
<!-- JS Page Level -->
<script type="text/javascript" src="~/_Assets/js/app.js"></script>
<script type="text/javascript">
